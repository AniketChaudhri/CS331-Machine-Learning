      "source": [
        "#importing libraries\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import math\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import PolynomialFeatures\n",
        "from sklearn.linear_model import LinearRegression"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#combination function(required for finding coefficients of Legendre's polynomial)\n",
        "def comb(n, k):\n",
        "  return math.factorial(n)/(math.factorial(k)*math.factorial(n-k)) "
      ],
      "metadata": {
        "id": "52oKIOj47MBY"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "ASGAh87jKevG"
      },
      "outputs": [],
      "source": [
        "#Generating coefficients of required Legendre's polynomial and normalizing it\n",
        "def generate_coeff(order):\n",
        "  coeff = np.zeros((order+1,1)) #coefficients\n",
        "  for i in range(int(order/2)+1):\n",
        "    coeff[order - 2*i] = (-1)**i*comb(order, i)*comb(2*order - 2*i, order)/2**order\n",
        "    coeff = coeff/np.linalg.norm(coeff)\n",
        "    #coeff[2] = coeff[2] + 2\n",
        "  return coeff"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "3SCG619ZHOSo"
      },
      "outputs": [],
      "source": [
        "#function generating data points given sample size, coefficients of polynomial, order of polynomial, and whether to add noise or not(default 'no')\n",
        "def generate_data(size, coeff, order, add_err = False):\n",
        "  X = np.random.uniform(-7, 7, size)                                            #uniformly distributed X ~ U[-7,7]   \n",
        "  X = np.array( sorted( X ))\n",
        "  #X = X/max(X)\n",
        "  X = np.array([X**i for i in range(0,order+1)]).T                              #generate nth order polynomial features \n",
        "\n",
        "  coeff = coeff.reshape(-1,1)\n",
        "\n",
        "  y = X @ coeff                                                                 #generating 'y' \n",
        "  if(add_err == True): y = y + np.random.normal(0,0.1, size).reshape(-1,1)      #adding noise if required(sigma^2 is set to 0.1)\n",
        "  \n",
        "  return X[:,1], y"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "6XdNtadcuNz8"
      },
      "outputs": [],
      "source": [
        "#Function to compute lms loss\n",
        "def error(a, b):\n",
        "  return (1/2)*(np.linalg.norm(a-b))**2"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "ZrXb3-IcJwTv"
      },
      "outputs": [],
      "source": [
        "#defining function for fitting polynomial of given order given train and test data\n",
        "def poly_fit(data, order, y, dtest, ytest):\n",
        "  poly = PolynomialFeatures(degree = order)\n",
        "  X_poly = poly.fit_transform(data)\n",
        "  poly.fit(X_poly, y)\n",
        "  lin2 = LinearRegression()                                                     #using sklearn for polynomial fitting\n",
        "  lin2.fit(X_poly, y)\n",
        "  y_pred = lin2.predict(poly.fit_transform(dtest))\n",
        "  Ein = error(y, lin2.predict(poly.fit_transform(data)))\n",
        "  Eout = error(y_pred, ytest)                                                   #calculating and returning errors(train and test)\n",
        "  return Ein, Eout\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "NkwoTumcktya"
      },
      "outputs": [],
      "source": [
        "#defining caltech1 function which will generate data as per need and will calculate expected values of out of sample and in sample errors\n",
        "def caltech1(poly_order, err, curve_order):\n",
        "  c = generate_coeff(poly_order)                                                #generating coefficients \n",
        "  E1 = []\n",
        "  E2 = []\n",
        "  for n in range(20, 1000, 50):\n",
        "    ein = []\n",
        "    eout = []\n",
        "    for j in range(1000):\n",
        "      x, y = generate_data(n, c, poly_order, add_err = err)                     #generating data\n",
        "      x,y = x.reshape(-1,1), y.reshape(-1,1)\n",
        "      X_train, X_test, y_train, y_test = train_test_split(x, y, test_size=0.50, random_state=42) #train-test split\n",
        "      Ein, Eout = poly_fit(X_train, curve_order, y_train, X_test, y_test)       #fitting\n",
        "      ein.append(Ein)\n",
        "      eout.append(Eout)                                                         \n",
        "    a = np.mean(ein)/n                                                          #mean error per sample point(as we are increasing sample points we will take error per point)\n",
        "    b = np.mean(eout)/n\n",
        "    E1.append(a)\n",
        "    E2.append(b)\n",
        "  return E1, E2\n",
        "    #print(n)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#plots for order 10 noisy data and quadratic fitting\n",
        "E1, E2 = caltech1(10, True, 2)\n",
        "plt.plot(range(20, 1000, 50),E1, label= 'In-sample error')\n",
        "plt.plot(range(20, 1000, 50),E2, label= 'Out-of-sample error')\n",
        "plt.legend()\n",
        "plt.show()"
      ],
